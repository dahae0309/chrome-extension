{
  "manifest_version": 3,
  "name": "CopyAndPaste",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "copy.png"
  },

  "content_scripts": [
    {
      "js": [
        "scripts/content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],

  "permissions": [
    "tabs",
    "storage",
    "<all_urls>",
    "scripting",
    "activeTab"
  ]
}